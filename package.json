{
  "name": "@vuetify/indexer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx --env-file=.env --watch src/index.ts",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "collect:api": "tsx --env-file=.env src/tasks/build-vuetify-api.ts",
    "collect:examples": "tsx --env-file=.env src/tasks/process-vuetify-examples.ts",
    "collect:all": "pnpm collect:api && pnpm collect:examples",
    "analyze:snippet": "tsx --env-file=.env src/demo/analyze-vue-snippet.ts",
    "analyze:batch": "tsx --env-file=.env src/demo/batch-analyze-snippets.ts",
    "analyze:file": "tsx --env-file=.env src/demo/analyze-file-content.ts",
    "data:search": "tsx --env-file=.env src/search-vectors.ts",
    "data:process": "tsx --env-file=.env src/index.ts --verbose",
    "data:clean": "tsx --env-file=.env src/delete-vectors.ts",
    "explore:api": "tsx --env-file=.env src/demo/explore-vuetify-api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@pinecone-database/pinecone": "^5.1.1",
    "@types/node": "^22.14.1",
    "consola": "^3.4.2",
    "dotenv": "^16.5.0",
    "glob": "^11.0.1",
    "openai": "^4.95.1",
    "ora": "^8.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "oxlint": "^0.16.7",
    "tsx": "^4.19.3",
    "upath": "^2.0.1"
  }
}
